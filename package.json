  "peerDependencies": {
    "@tensorflow/tfjs": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tensorflow/tfjs": "^4.22.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.18"
  }
