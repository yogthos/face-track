{
  "name": "face-track",
  "version": "0.1.2",
  "type": "module",
  "description": "Real-time face landmark detection and tracking using TensorFlow.js",
  "main": "dist/face-track.js",
  "module": "dist/face-track.js",
  "exports": {
    ".": {
      "import": "./dist/face-track.js"
    }
  },
  "files": [
